SELECT * FROM `_Tên_Bảng_`\";\r\n    list = db_table(sql)\r\n    \r\n    print(\"Danh Sách: \");\r\n    \r\n    columns();\r\n    \r\n    for obj in list:\r\n        row(obj);\r\n#}\r\n\r\n# Hiện các cột của bảng\r\ndef columns(): \r\n#{\r\n    print(\"+-------------------------------------------------+\");\r\n    print(\"| ID | CỘT_1 | CỘT_2| CỘT_3 |\");\r\n    print(\"+-------------------------------------------------+\");\r\n#} kết thúc hàm hiện cột của bảng\r\n\r\n# Hiện một dòng bản ghi dữ liệu\r\ndef row(obj): \r\n#{\r\n    print(\"| %s | %13s | %8s | %15s |\" \r\n        %(obj[\'_Khóa_Chính_Là_Gì_\'],obj[\'_Cột_1_db_\'],obj[\'_Cột_2_db_\'],obj[\'_Cột_3_db_\']));\r\n    print(\"+-------------------------------------------------+\");\r\n#}  kết thúc hàm hiện dòng\r\n\r\n# Sắp xếp danh sách \r\ndef sort(): \r\n#{\r\n    print(\"Danh sách tăng dần theo ___: \");\r\n    sql = \"SELECT * FROM `_Tên_Bảng_` ORDER BY `_Tên_Cột_Sắp_Xếp_`\";\r\n    \r\n    #print(\"Danh sách giảm dần theo ___: \");\r\n    #sql = \"SELECT * FROM `_Tên_Bảng_` ORDER BY `_Tên_Cột_Sắp_Xếp_` DESC\";\r\n    list = db_table(sql)\r\n    \r\n    columns();\r\n    \r\n    for obj in list:\r\n        row(obj);\r\n#}  kết thúc hàm sắp xếp điểm giảm dần\r\n\r\n# Thống kê, ví dụ: số sinh viên theo năm sinh\r\ndef analyze(): \r\n#{\r\n    sql = \"\"\"\r\n        SELECT `_Cột_Thống_Kê_`,\r\n               COUNT(`_Cột_Khóa_Chính_`) AS `count`\r\n        FROM `_Tên_Bảng_`\r\n        GROUP BY `_Cột_Thống_Kê_`\r\n    \"\"\";\r\n    list = db_table(sql)\r\n    \r\n    columns();\r\n    \r\n    for obj in list:\r\n        print(\"Có %s đối tượng thuộc loại %s\" %(obj[\'count\'],obj[\'_Cột_Thống_Kê_\']));\r\n#} kết thúc hàm thống kê\r\n\r\n# Tìm kiếm\r\ndef search(): \r\n#{\r\n    \r\n    keyword = input(\"Nhập từ khóa: \");\r\n    \r\n    # ví dụ: SELECT * FROM `student` WHERE `name` LIKE \'ung\'\r\n    sql = \"SELECT * FROM `_Tên_Bảng_` WHERE `_Cột_Tìm_Kiếm_` LIKE \'%s\'\" % (\'%\'+keyword+\'%\')\r\n    list = db_table(sql);\r\n    \r\n    if len(list) < 1:\r\n        print(\"Không tìm thấy !\");\r\n    else:\r\n        columns();\r\n    \r\n        for obj in list:\r\n            row(obj);\r\n        \r\n        print(\"Tìm thấy %s đối tượng khớp với từ khóa \'%s\'\" %(len(list),keyword));\r\n        \r\n#}\r\n\r\ndef die():\r\n#{\r\n    print(\"Cảm ơn bạn đã sử dụng phần mềm! Tạm biệt và hẹn gặp lại!\");\r\n    sys.exit();\r\n#}'),
