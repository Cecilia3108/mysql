(109, 'file class.UiGridCode.php, mã điều khiển UiGrid', 'file class.UiGridCode.php, ma dieu khien UiGrid', '<?php\r\n\r\n// Gọi lớp giao diện cha\r\ninclude_once \'class.UiGrid.php\';\r\n\r\n// Gọi các lớp giao diện khác nếu cần\r\n//include_once \'class.UiAddCode.php\';\r\n//include_once \'class.UiEditCode.php\';\r\n\r\n// Gọi thư viện thao tác cơ sở dữ liệu\r\ninclude_once \'db.php\'; \r\n\r\nclass UiGridCode extends UiGrid\r\n{\r\n	// Biến theo dõi dòng nào đang được bôi đen\r\n	var $row_index_selected = -1;\r\n	\r\n	function __construct()\r\n	{\r\n		parent::__construct();\r\n		\r\n		// Đặt lại tên các cột trên Grid\r\n		$this->uiGrid->SetColLabelValue(0, \"sửa_Mã_Khóa_Chính\");\r\n		$this->uiGrid->SetColLabelValue(1, \"sửa_Cột_2\");\r\n		$this->uiGrid->SetColLabelValue(2, \"sửa_Cột_3\");\r\n		$this->uiGrid->SetColLabelValue(3, \"sửa_Cột_4\");\r\n		\r\n		// Tải dữ liệu lên Grid\r\n		$this->load();\r\n		\r\n	}\r\n	\r\n	function load()\r\n	{\r\n		// Xóa hết các dòng cũ trước khi tải lại\r\n		$rows_count = $this->uiGrid->GetNumberRows();\r\n		if ($rows_count > 0)\r\n			$this->uiGrid->DeleteRows(0,$rows_count);	// bắt đầu xóa từ dòng có index=0\r\n		\r\n		// Truy vấn dữ liệu từ db\r\n		$sql = \"SELECT * FROM `sửa_Tên_Bảng`\";\r\n		$list = db_q($sql);\r\n   \r\n		// Tải dữ liệu lên GridView\r\n		$row_index = 0;\r\n		foreach($list as $obj)\r\n		{\r\n			// Đổ dữ liệu lên dòng\r\n			$this->uiGrid->AppendRows(1);\r\n			$this->uiGrid->SetCellValue($row_index,0,$obj[\"sửa_Cột_Khóa_Chính_db\"]);\r\n			$this->uiGrid->SetCellValue($row_index,1,$obj[\"sửa_Cột_2_db\"]);\r\n			$this->uiGrid->SetCellValue($row_index,2,$obj[\"sửa_Cột_3_db\"]);\r\n			$this->uiGrid->SetCellValue($row_index,3,$obj[\"sửa_Cột_4_db\"]);\r\n			\r\n			// Ngăn không cho sửa dòng này...\r\n			// (chú ý đếm xem có bao nhiêu cột 4->0,1,2,3)\r\n			for ($col_index=0; $col_index < 4; $col_index++)\r\n			{\r\n				// ...bằng cách disable từng cell một trên dòng này \r\n				$this->uiGrid->SetReadOnly($row_index, $col_index, true);\r\n			}\r\n			\r\n			$row_index++;\r\n		}\r\n		\r\n		// Bôi đen dòng đầu tiên sau khi tải dữ liệu lên\r\n		$this->uiGrid->SelectRow(0);\r\n\r\n		// Điều chỉnh cột và dòng cho vừa với nội dung\r\n		$this->uiGrid->AutoSize();\r\n		// Cho phép bôi đen nhiều dòng:\r\n		// (phục vụ tính năng xóa/copy nhiều dòng cùng lúc)\r\n		$this->uiGrid->SetSelectionMode( wxGrid::wxGridSelectRows );\r\n	}\r\n	\r\n	// Thoát chương trình\r\n	function OnMenuSelection_Exit( $event )\r\n	{\r\n		$event->Skip();\r\n		$this->Destroy();\r\n	}\r\n	\r\n	// Giới thiệu về phần mềm\r\n	function OnMenuSelection_About( $event )\r\n	{\r\n		$event->Skip();\r\n		wxMessageBox(\"Cảm ơn bạn đã dùng phần mềm ! \\n (C)2020 sửa_Tên_Tác_Giả\",\"About Me\");\r\n	}\r\n	\r\n	// Thêm mới bản ghi\r\n	function OnButtonClick_Add( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		$ui_add = new UiAddCode();\r\n		$ui_add->Show();\r\n	}\r\n	\r\n	// Sửa bản ghi\r\n	function OnButtonClick_Edit( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		// Kiểm tra xem Grid có dòng bản ghi nào không \r\n		if ($this->uiGrid->GetNumberRows() < 1)\r\n			wxMessageBox(\"Lỗi-Không có dòng bản ghi nào !\");\r\n		\r\n		// Kiểm tra xem có dòng nào được bôi đen không\r\n		if ($this->row_index_selected < 0)\r\n			wxMessageBox(\"Lỗi-Bạn vui lòng chọn dòng để sửa !\");\r\n		\r\n		// Lấy ra khóa chính của dòng\r\n		// phải nhìn xem khóa chính được đặt ở cột nào\r\n		// (ví dụ:, Mã Sinh Viên đặt ở cột đầu tiên, chỉ số=0)\r\n		$pk = $this->uiGrid->GetCellValue($this->row_index_selected,0);\r\n		\r\n		// Truyền khóa chính sang cho giao diện UiEdit \r\n		$ui_edit = new UiEditCode($pk);\r\n		$ui_edit->Show();\r\n	}\r\n	\r\n	// Xóa bản ghi\r\n	function OnButtonClick_Delete( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		// Kiểm tra xem Grid có dòng bản ghi nào không \r\n		if ($this->uiGrid->GetNumberRows() < 1)\r\n			wxMessageBox(\"Lỗi-Không có dòng bản ghi nào !\");\r\n		\r\n		// Kiểm tra xem có dòng nào được bôi đen không\r\n		if ($this->row_index_selected < 0)\r\n			wxMessageBox(\"Lỗi-Bạn vui lòng chọn dòng để xóa !\");\r\n		\r\n		// Lấy ra khóa chính của dòng\r\n		// phải nhìn xem khóa chính được đặt ở cột nào\r\n		// (ví dụ:, Mã Sinh Viên đặt ở cột đầu tiên, chỉ số=0)\r\n		$pk = $this->uiGrid->GetCellValue($this->row_index_selected,0);\r\n		\r\n		$yn = wxMessageBox(\"Bạn có chắc muốn xóa ?\", \"Xác Nhận\", wxYES_NO, $this);\r\n		\r\n		// Nếu người dùng đồng ý xóa bản ghi\r\n		if ($yn == wxYES)\r\n		{\r\n			$sql = \"DELETE FROM `sửa_Tên_Bảng` WHERE `sửa_Tên_Khóa_Chính`=\'{$pk}\'\";\r\n			\r\n			db_q($sql);\r\n			\r\n			wxMessageBox(\"Đã hoàn thành việc xóa !\", \"Thành Công\", wxOK, $this);\r\n		}\r\n	}\r\n	\r\n	// Thoát ứng dụng\r\n	function OnButtonClick_Exit( $event )\r\n	{\r\n		$event->Skip();\r\n		$this->Close();\r\n	}\r\n	\r\n	// Làm tươi mới/cập nhật giao diện\r\n	function OnClick_ButtonRefresh( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		$this->load();\r\n	}\r\n	\r\n	// Ngay sau khi một dòng được bôi đen thì lưu lại index của nó\r\n	function OnGridSelectCell_uiGrid( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		$this->row_index_selected = $event->GetRow();\r\n	}\r\n	\r\n	// Chọn nhiều bản ghi cùng lúc\r\n	function OnGridRangeSelect_uiGrid( $event )\r\n	{\r\n		$event->Skip();\r\n	}\r\n	\r\n	// Nhấp đúp vào một dòng trên Grid để sửa\r\n	function OnGridDoubleClick( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		$this->OnGridSelectCell_uiGrid($event);\r\n		$this->OnButtonClick_Edit($event);\r\n	}\r\n	\r\n	// Khi UiGrid trở lại trạng thái \'Tích Cực\'\r\n	// (tức là được tương tác trở lại sau khi cửa sổ khác khép lại)\r\n	// thì tải lại dữ liệu mới cập nhật (add/edit)\r\n	function OnActivate_UiGrid( $event )\r\n	{\r\n		$event->Skip();\r\n		$this->load();\r\n	}\r\n	\r\n	// Nhấp chuột phải vào Grid thì hiện menu ngữ cảnh\r\n	function OnGridRightClick( $event )\r\n	{\r\n		$event->Skip();\r\n		\r\n		// Bôi đen dòng đang nhấp chuột phải\r\n		$this->uiGrid->SelectRow($event->GetRow());\r\n		$this->OnGridSelectCell_uiGrid($event); \r\n		\r\n		// Tạo menu ngữ cảnh (Context Menu)\r\n		$point = $event->GetPosition();\r\n		\r\n		$menu = new wxMenu();\r\n		$menu_Add    = new wxMenuItem( $menu, wxID_ANY, \"Thêm\", wxEmptyString, wxITEM_NORMAL );\r\n		$menu_Edit   = new wxMenuItem( $menu, wxID_ANY, \"Sửa\",  wxEmptyString, wxITEM_NORMAL );\r\n		$menu_Delete = new wxMenuItem( $menu, wxID_ANY, \"Xóa\",  wxEmptyString, wxITEM_NORMAL );\r\n		\r\n\r\n		$menu->Append($menu_Add);\r\n		$menu->Append($menu_Edit);\r\n		$menu->Append($menu_Delete);\r\n		\r\n		// Kết nối menu với các hàm xử lý sự kiện\r\n		// Đừng đặt đọa mã này dưới lệnh PopupMenu() vì nó sẽ ko chạy hàm sự kiện\r\n		$this->Connect( $menu_Add->GetId(), wxEVT_COMMAND_MENU_SELECTED, array($this, \"OnButtonClick_Add\") );\r\n		$this->Connect( $menu_Edit->GetId(), wxEVT_COMMAND_MENU_SELECTED, array($this, \"OnButtonClick_Edit\") );\r\n		$this->Connect( $menu_Delete->GetId(), wxEVT_COMMAND_MENU_SELECTED, array($this, \"OnButtonClick_Delete\") );\r\n\r\n		// Sau cùng mới là hiện menu ngữ cảnh\r\n		$this->PopupMenu( $menu, $point);\r\n		\r\n	}\r\n	\r\n} // kết thúc lớp');
