(14, 'FormEdit.cs (Employee, Nhân Sự)', 'FormEdit.cs (Employee, Nhan Su)', 'using System;\r\nusing System.Collections.Generic;\r\nusing System.ComponentModel;\r\nusing System.Data;\r\nusing System.Drawing;\r\nusing System.Linq;\r\nusing System.Text;\r\nusing System.Threading.Tasks;\r\nusing System.Windows.Forms;\r\n\r\nnamespace sửa_Tên_Dự_Án_đi\r\n{\r\n    public partial class FormEdit : Form\r\n    {\r\n        public FormEdit()\r\n        {\r\n            InitializeComponent();\r\n        }\r\n\r\n        public Employee Row { get; set; }\r\n\r\n        DBEntities db = new DBEntities();\r\n\r\n        protected override void OnShown(EventArgs e)\r\n        {\r\n            base.OnShown(e);\r\n\r\n            // Tải dữ liệu cần sửa lên các Control khác:\r\n            this.uiTextBox_Name.Text = this.Row.EmployeeName;\r\n            this.uiTextBox_Tel.Text = this.Row.Tel;\r\n            this.uiTextBox_Addr.Text = this.Row.Address;\r\n            this.uiDateTime_BirthDate.Value = this.Row.BirthDate.Value;\r\n            this.uiRadioButton_Male.Checked = this.Row.Gender;\r\n            this.uiRadioButton_Female.Checked = !this.Row.Gender;\r\n            this.uiComboBox_Dept.SelectedValue = this.Row.Department.DeptID;\r\n\r\n            // Đăng kí xử lý sự kiện\r\n            this.uiButton_Cancel.Click += OnClick_Cancel;\r\n            this.uiButton_Save.Click += OnClick_Save;\r\n\r\n        }\r\n\r\n        private void OnClick_Save(object sender, EventArgs e)\r\n        {\r\n            // Lấy dữ liệu trên FormEdit xuống\r\n            var row = this.db.Employees.ToList().Find(obj => obj.EmployeeID == this.Row.EmployeeID);\r\n            row.EmployeeName = this.uiTextBox_Name.Text;\r\n            row.Tel = this.uiTextBox_Tel.Text;\r\n            row.Address = this.uiTextBox_Addr.Text;\r\n            row.DeptID = this.uiComboBox_Dept.SelectedValue.ToString();\r\n            row.BirthDate = this.uiDateTime_BirthDate.Value;\r\n            row.Gender = this.uiRadioButton_Male.Checked;\r\n\r\n            // Lưu vào db\r\n            this.db.Entry(row).State = System.Data.Entity.EntityState.Modified;\r\n            db.SaveChanges();\r\n\r\n            MessageBox.Show(\"Đã sửa thành công !\", \"thành công\", MessageBoxButtons.OK);\r\n\r\n            // đóng FormEdit\r\n            this.Close();\r\n        }\r\n\r\n        private void OnClick_Cancel(object sender, EventArgs e)\r\n        {\r\n            this.Close();\r\n        }\r\n\r\n        protected override void OnClosed(object sender, EventArgs e)\r\n        {\r\n            //tải lại FormList\r\n            (this.Owner as FormList).loadTable(this.db.Employees.ToList());\r\n        }\r\n\r\n    }\r\n}\r\n'),
