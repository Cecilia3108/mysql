-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `code4php`
--

DROP TABLE IF EXISTS `code4php`;
CREATE TABLE IF NOT EXISTS `code4php` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `tag` text,
  `content` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=171 DEFAULT CHARSET=utf8;

--
-- Đang đổ dữ liệu cho bảng `code4php`
--

