(16, 'FormList.cs (Mã Điều Khiển)', 'FormList.cs (Code Behind)', 'using System;\r\nusing System.Collections.Generic;\r\nusing System.ComponentModel;\r\nusing System.Data;\r\nusing System.Drawing;\r\nusing System.Linq;\r\nusing System.Text;\r\nusing System.Threading.Tasks;\r\nusing System.Windows.Forms;\r\n\r\nnamespace sửa_Tên_Dự_Án_đi\r\n{\r\n    public partial class FormList : Form\r\n    {\r\n        public FormList()\r\n        {\r\n            InitializeComponent();\r\n        }\r\n\r\n\r\n        TÊN_BẢNG_trên_ListView Row { get; set; }\r\n        DBEntities db = new DBEntities();\r\n\r\n        protected override void OnShown(EventArgs e)\r\n        {\r\n            base.OnShown(e);\r\n\r\n            foreach (var item in this.db.TÊN_BẢNG_SỐ_NHIỀU_trên_TreeView)\r\n            {\r\n                var node = new TreeNode();\r\n                node.Name = item.KHÓA_CHÍNH_BẢNG_TreeView;\r\n                node.Text = item.CỘT_HIỂN_THỊ_trên_TreeView;\r\n                this.uiTreeView.Nodes.Add(node);\r\n            }\r\n\r\n            this.loadTable();\r\n\r\n            this.uiTreeView.AfterSelect += OnAfterSelect_TreeView;\r\n            this.uiListView.Click += OnClick_ListView;\r\n\r\n            this.uiButton_Insert.Click += OnClick_Insert;\r\n            this.uiButton_Delete.Click += OnClick_Delete;\r\n            this.uiButton_Close.Click += OnClick_Close;\r\n        }\r\n\r\n        public void loadTable(List<TÊN_BẢNG_trên_ListView> list = null)\r\n        {\r\n\r\n            this.uiListView.View = View.Details;\r\n            this.uiListView.FullRowSelect = true;\r\n            this.uiListView.Clear();\r\n\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_ListView.KHÓA_CHÍNH), Text = \"Khóa Chính\", Width = 100 });\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_ListView.CỘT_2), Text = \"Tên_Cột_2_trên_giao_diện\", Width = 100 });\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_TREE_VIEW.CỘT_3_ví_dụ_DeptName), Text = \"Tên_Cột_3_trên_giao_diện\", Width = 100 });\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_ListView.CỘT_4), Text = \"Tên_Cột_4_trên_giao_diện\", Width = 100 });\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_ListView.CỘT_5), Text = \"Tên_Cột_5_trên_giao_diện\", Width = 100 });\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_ListView.CỘT_6), Text = \"Tên_Cột_6_trên_giao_diện\", Width = 100 });\r\n            this.uiListView.Columns.Add(new ColumnHeader() { Name = nameof(TÊN_BẢNG_trên_ListView.CỘT_7), Text = \"Tên_Cột_7_trên_giao_diện\", Width = 100 });\r\n\r\n            if (list == null)\r\n                list = this.db.TÊN_BẢNG_trên_ListViews.ToList();\r\n\r\n            foreach (var obj in list)\r\n            {\r\n\r\n                var item = new ListViewItem(obj.KHÓA_CHÍNH_BẢNG_trên_ListView.ToString());\r\n                item.SubItems[0].Name = nameof(TÊN_BẢNG_trên_ListView.KHÓA_CHÍNH);\r\n\r\n                item.SubItems.Add(obj.CỘT_2);\r\n                item.SubItems.Add(obj.TÊN_BẢNG_trên_TreeView.CỘT_3);\r\n                item.SubItems.Add(obj.Gender ? \"Nam\" : \"Nữ\");\r\n                item.SubItems.Add(obj.BirthDate.Value.ToString(\"dd/MM/yyyy\"));\r\n                item.SubItems.Add(obj.CỘT_6);\r\n                item.SubItems.Add(obj.CỘT_7);\r\n\r\n                this.uiListView.Items.Add(item);\r\n                this.uiListView.AutoResizeColumns(ColumnHeaderAutoResizeStyle.ColumnContent);\r\n            }\r\n\r\n            selectRow(0);\r\n\r\n        }\r\n\r\n        void selectRow(int index)\r\n        {\r\n\r\n            this.uiListView.SelectedItems.Clear();\r\n            this.uiListView.Items[index].Selected = true;\r\n            this.uiListView.Items[index].Focused = true;\r\n            this.uiListView.Select();\r\n\r\n            this.uiButton_Delete.Enabled = true;\r\n\r\n            var id = this.uiListView.SelectedItems[0].SubItems[nameof(TÊN_BẢNG_trên_ListView.KHÓA_CHÍNH)].Text;\r\n            this.Row = this.db.TÊN_BẢNG_trên_ListViews.ToList().Find(obj => obj.KHÓA_CHÍNH.ToString() == id);\r\n        }\r\n\r\n        private void OnClick_Close(object sender, EventArgs e)\r\n        {\r\n            this.Close();\r\n        }\r\n\r\n        private void OnClick_ListView(object sender, EventArgs e)\r\n        {\r\n            this.selectRow(this.uiListView.FocusedItem.Index);\r\n        }\r\n\r\n        private void OnAfterSelect_TreeView(object sender, TreeViewEventArgs e)\r\n        {\r\n            var list = this.db.TÊN_BẢNG_trên_ListViews.Where(obj => obj.TÊN_BẢNG_trên_TreeView.CỘT_HIỂN_THỊ_trên_TreeView == this.uiTreeView.SelectedNode.Text).ToList();\r\n            if (list == null || (list != null && list.Count <= 0))\r\n            {\r\n                MessageBox.Show($\"Không tìm thấy bản ghi nào khớp với \'{this.uiTreeView.SelectedNode.Text}\' !\");\r\n                return;\r\n            }\r\n\r\n            this.loadTable(list);\r\n        }\r\n\r\n        private void OnClick_Delete(object sender, EventArgs e)\r\n        {\r\n            if (this.uiListView.SelectedItems.Count <= 0)\r\n            {\r\n                MessageBox.Show(\"Bạn vui lòng chọn dòng để xóa\", \"Lỗi\", MessageBoxButtons.OK, MessageBoxIcon.Error);\r\n                return;\r\n            }\r\n\r\n            var yes_no = MessageBox.Show(\"Bạn có chắc muốn xóa ?\", \"Xác Nhận\", MessageBoxButtons.YesNo, MessageBoxIcon.Warning);\r\n\r\n            if (yes_no == DialogResult.Yes)\r\n            {\r\n                this.db.Entry(this.Row).State = System.Data.Entity.EntityState.Deleted;\r\n                this.db.SaveChanges();\r\n\r\n                MessageBox.Show(\"Đã hoàn tất việc xóa!\", \"Thành Công\", MessageBoxButtons.OK, MessageBoxIcon.Information);\r\n\r\n                this.loadTable();\r\n            }\r\n        }\r\n\r\n        private void OnClick_Insert(object sender, EventArgs e)\r\n        {\r\n            var f = new FormAdd();\r\n            f.Owner = this;\r\n            f.Show();\r\n        }\r\n\r\n    }\r\n}\r\n'),
