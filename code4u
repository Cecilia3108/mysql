(143, 'hàm analyze() thống kê theo chuỗi', 'ham analyze() thong ke theo chuoi', 'function analyze()\r\n{\r\n	global $list;\r\n		\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(list_empty()) \r\n		return;\r\n\r\n	// Nếu danh sách đã được nhập thì tiến hành thống kê\r\n\r\n	$sl = array();	// Mảng chứa thông tin thống kê\r\n\r\n	// Ban đầu mỗi phần tử có giá trị là 1\r\n	// bởi vì mỗi phần tử xuất hiện ít nhất một lần trong mảng\r\n	for ($i=0; $i<count($list); $i++)\r\n	{\r\n		$sl[$i]=1;\r\n	}\r\n\r\n	for ($i=0; $i<count($list);$i++)\r\n	{\r\n		// Với mỗi phần tử [i]\r\n		// so sánh với các phần tử còn lại tính từ [i+1]\r\n		for ($j=$i+1; $j<count($list); $j++)\r\n		{\r\n			// Nếu thống kê của [j] khác không\r\n			if ($sl[$j]!=0)\r\n			{\r\n				// và [i], [j] cùng loại\r\n				if (strcmp($list[$i]->blood,$list[$j]->blood)==0)\r\n				{\r\n					$sl[$i]++;	// thì tăng thống kê của [i] lên một đơn vị\r\n					$sl[$j]-=1;	// và thiết lập [j] về không\r\n				}\r\n			}\r\n		}\r\n\r\n	}\r\n\r\n	echo \"\\n Statistics Results:\";\r\n	for ($i=0; $i<count($list); $i++)\r\n	{\r\n		if ($sl[$i]!=0)\r\n		{\r\n			printf(\"\\n+ Blood Type \'%s\' : %d pets\", $list[$i]->blood, $sl[$i]);\r\n		}\r\n	}\r\n} // end analyze()'),
(144, 'hàm find() tìm lớn nhất max', 'tim lon nhat max', 'function find()\r\n{\r\n	global $list;\r\n		\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(list_empty()) \r\n		return;\r\n\r\n	// Tìm ra tuổi nhỏ nhất, tương đương năm sinh lớn nhất.\r\n	$max_age = $list[0]->age;\r\n\r\n	for ($i=0; $i<count($list); $i++)\r\n	{\r\n		if ($max_age < $list[$i]->age)\r\n		{\r\n			$max_age = $list[$i]->age;\r\n		}\r\n	}\r\n\r\n	// Hiển thị các đối tượng có tuổi bằng tuổi nhỏ nhất\r\n	echo \"\\n Pets oldest: \";\r\n	col();\r\n	for ($i=0; $i<count($list); $i++)\r\n	{\r\n		if ($max_age == $list[$i]->age)\r\n		{\r\n			row($list[$i],$i);\r\n		}\r\n	}\r\n}');
