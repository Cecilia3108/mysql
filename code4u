c\':\r\n				system(\"cls\");\r\n				break;\r\n			case \'f\':\r\n				system(\"mode 800,800\"); // Chuyển khung nhìn sang chế độ toàn màn hình\r\n				goto menu;			// Tiếp tục bước lặp tiếp theo (quay lại menu ban đầu).\r\n			default:\r\n				echo \"\\n Lựa chọn không hợp lệ ! Bạn vui lòng nhập từ [0-7] hoặc \'y\', \'n\', \'Y\', \'N\'\";\r\n				break;\r\n\r\n		} // kết thúc luồng xử lý rẽ nhánh switch case\r\n\r\n		echo \"\\n Bạn có muốn tiếp tục không ? (y/n):\";\r\n		goto input_choice;\r\n\r\n} // kết thúc vòng lặp while() chương trình chính\r\n	\r\n\r\n'),
