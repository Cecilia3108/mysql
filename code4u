n		UpdateWindow(hwnd);\r\n\r\n		MSG Msg;\r\n\r\n		// Step 3: The Message Loop\r\n		while(GetMessage(&Msg, NULL, 0, 0) > 0)\r\n		{\r\n			TranslateMessage(&Msg);\r\n			DispatchMessage(&Msg);\r\n		}\r\n	}\r\n	\r\n\r\n}; // end class'),
