 lớp \'Content\' lên giao diện html, \r\nví dụ: file _Layout.cshtml\r\n\r\n	@using TÊN_DỰ_ÁN.Translations <!-- Gọi không gian tên miền chứa lớp dịch thuật Content -->\r\n\r\n	<!DOCTYPE html>\r\n	...\r\n	...\r\n	<ul class=\"nav navbar-nav\">\r\n		<li>@Html.ActionLink(Content.Home, \"Index\", \"Home\")</li> <!-- Tải chuỗi dịch thuật -->\r\n		<li>@Html.ActionLink(Content.About, \"About\", \"Home\")</li>\r\n		<li>@Html.ActionLink(Content.Contact, \"Contact\", \"Home\")</li>\r\n	</ul>\r\n	...\r\n	...\r\n\r\n+-----------------------------------------+\r\n| 6. DỊCH THUẬT CHO Controller            |\r\n+-----------------------------------------+\r\nMặc dù cấu hình ngôn ngữ trong Web.config là tiếng Anh Mĩ (en-US),\r\nnhưng bạn vẫn có thể đổi sang tiếng Nhật ngay bên trong controller.\r\nVí dụ: \r\n\r\nnamespace WAD_i18n.Controllers\r\n{\r\n	  \r\n	using System.Globalization;\r\n    using System.Threading;\r\n	\r\n    public class i18nController : Controller\r\n    {\r\n        public ActionResult Index(string locale)\r\n        {\r\n            // ...\r\n            \r\n            if (!String.IsNullOrEmpty(Request[\"locale\"]))\r\n            {\r\n                Thread.CurrentThread.CurrentCulture = new CultureInfo(Request[\"locale\"]);\r\n                Thread.CurrentThread.CurrentUICulture = new CultureInfo(Request[\"locale\"]);\r\n            }\r\n			\r\n			....\r\n            return View();\r\n        }\r\n \r\n    }// end class\r\n}\r\n///////////////////////////////////////\r\nMã nguồn bên View Index()\r\n\r\n<form action=\"Index\" method=\"get\">\r\n    <select id=\"locale\" name=\"locale\">\r\n        <option value=\"en-US\" @(\"en-US\" == @System.Globalization.CultureInfo.CurrentCulture.Name ? \"selected\" : \"\")>English (US)</option>\r\n        <option value=\"vi-VN\" @(\"vi-VN\" == @System.Globalization.CultureInfo.CurrentCulture.Name ? \"selected\" : \"\")>Tiếng Việt Nam</option>\r\n        <option value=\"ja-JP\" @(\"ja-JP\" == @System.Globalization.CultureInfo.CurrentCulture.Name ? \"selected\" : \"\")>日本語</option>\r\n    </select>\r\n</form>\r\n<script>\r\n    document.querySelector(\'[name=\"pageSize\"]\').onchange = function () {\r\n        document.querySelector(\'form\').submit();\r\n    }\r\n\r\n    document.querySelector(\'[name=\"locale\"]\').onchange = function () {\r\n        document.querySelector(\'form\').submit();\r\n    }\r\n</script>\r\n\r\n<table>\r\n    <caption>Ngôn Ngữ Hiện Tại Của Hệ Thống</caption>\r\n    <thead>\r\n        <tr>\r\n            <th>Mã Phương Ngữ</th>\r\n            <th>Văn Hóa</th>\r\n            <th>Định Dạng Ngày Tháng</th>\r\n            <th>Nói \"Xin Chào!\" như thế nào ?</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n\r\n        <tr>\r\n            <td>@System.Globalization.CultureInfo.CurrentCulture.Name</td>\r\n            <td>@System.Globalization.CultureInfo.CurrentCulture.DisplayName</td>\r\n            <td>@System.DateTime.Now.ToLongDateString()</td>\r\n            <td>@Content.Greeting</td>\r\n        </tr>\r\n\r\n\r\n    </tbody>\r\n</table>\r\n\r\n<br>\r\n\r\n<table>\r\n    <caption>Danh Sách Ngôn Ngữ Hỗ Trợ Bởi Hệ Thống</caption>\r\n    <thead>\r\n        <tr>\r\n            <th>Mã Phương Ngữ</th> <th>Văn Hóa</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        @foreach (var culture in @System.Globalization.CultureInfo.GetCultures(@System.Globalization.CultureTypes.AllCultures))\r\n        {\r\n            <tr>\r\n                <td>@culture.Name</td> <td>@culture.DisplayName</td>\r\n            </tr>\r\n        }\r\n\r\n    </tbody>\r\n</table>\r\n \r\n*/'),
