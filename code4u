(85, 'cấu trúc dữ liệu và giải thuật, cau truc du lieu va giai thuat, dữ liệu và hàm, du lieu va ham', 'dữ liệu và hàm, class var data and functions', '\r\n\r\n// Cấu trúc dữ liệu\r\nclass _Tên_Lớp_\r\n{\r\n	var $biến_1;\r\n	var $biến_2;\r\n	var $biến_3;\r\n	var $biến_4;\r\n} \r\n\r\n// Khởi tạo danh sách rỗng\r\n$danh_sach = array();\r\n\r\n// Giải thuật (hàm chức năng)\r\n\r\n// Hàm nhập dữ liệu\r\n// Kiểu trả về: void\r\nfunction nhap()\r\n{\r\n	global $danh_sach;\r\n	\r\n	if (!is_array($danh_sach))\r\n		$danh_sach = array();\r\n	\r\n	$dt = new _Tên_Lớp_();\r\n\r\n	echo \"\\n Nhập chuỗi: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$dt->tên_biến_chuỗi = rtrim(fgets($handle));\r\n\r\n	echo \"\\n Nhập số nguyên: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$dt->tên_biến_nguyên = (int)fgets($handle);\r\n\r\n	echo \"\\n Nhập số thực: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$dt->tên_biến_số_thực = (float)rtrim(fgets($handle));\r\n		\r\n	\r\n	// Thêm mới đối tượng vào danh sách cần quản lý\r\n	$danh_sach[] = $dt;\r\n}\r\n\r\n// Hàm kiểm tra xem danh sách có rỗng không\r\n// Kiểu trả về: boolean(true/false)\r\nfunction danh_sach_rong()\r\n{\r\n	global $danh_sach;\r\n	\r\n	if (!is_array($danh_sach) || empty($danh_sach))\r\n	{\r\n		echo \"\\n Lỗi! Danh sách chưa nhập!\";\r\n		return true;\r\n	}\r\n\r\n	return false;\r\n}\r\n\r\n// Hàm hiển thị các cột của bảng\r\n// STT: Số thứ tự\r\n// Kiểu trả về: void\r\nfunction hienCot()\r\n{\r\n	echo \"\\n+----------------------------------------------------+\";\r\n	echo \"\\n| STT | Cột_1     | Cột_2 | Cột_3 | Cột_4 |\";\r\n	echo \"\\n+----------------------------------------------------+\";\r\n}\r\n\r\n// Hàm hiển thị dữ liệu một dòng bản ghi\r\n// Tham số $dt: dữ liệu của một đối tượng\r\n// Tham số $stt: số thứ tự\r\nfunction hienDong($dt, $stt)\r\n{\r\n	printf(\"\\n|%-5s|%-15s|%-10s|%-11s|%7s|\", $stt, $dt->tên_biến, $dt->tên_biến, $dt->tên_biến);\r\n	echo \"\\n+----------------------------------------------------+\";\r\n}\r\n\r\n// Hàm xuất dữ liệu bảng\r\nfunction xuat()\r\n{\r\n	global $danh_sach;\r\n	\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(danh_sach_rong()) \r\n		return;\r\n\r\n	// Nếu đã nhập rồi thì tiến hành in ra màn hình\r\n	echo \"\\n Danh Sach:\";\r\n\r\n	// Hiện các cột của bảng\r\n	hienCot();\r\n\r\n	// sau đó hiện từng dòng bản ghi thông tin của mỗi đối tượng\r\n	for ($i=0; $i < count($danh_sach); $i++)\r\n	{\r\n		hienDong($danh_sach[$i],$i+1);\r\n	}\r\n}\r\n\r\n// function sap_xep(){}\r\n\r\n// function thong_ke(){}\r\n\r\n// function tim_kiem(){}\r\n\r\n\r\n// Hàm ghi dữ liệu vào file\r\n// tham khảo: \r\n// https://stackoverflow.com/questions/4997186/reading-a-php-array-saved-in-a-file\r\nfunction ghi_file()\r\n{\r\n	global $danh_sach;\r\n		\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(danh_sach_rong()) return;\r\n	\r\n	echo \"\\n Tên file: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$ten_file = rtrim(fgets($handle));	// C:\\danh_sach.txt\r\n\r\n	$fp=fopen($ten_file,\"w\"); // mở file để viết (write) \r\n	\r\n	if($fp==NULL)\r\n	{\r\n		echo \"\\n Không mở được file ! \\n\";\r\n	}\r\n	else\r\n	{\r\n		fwrite($fp, serialize($danh_sach));\r\n\r\n		echo \"\\n Đã ghi vào file thành công ! \\n\";\r\n	}\r\n\r\n	fclose($fp);\r\n}\r\n\r\nfunction doc_file()\r\n{\r\n	echo \"\\n Tên file: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$ten_file = rtrim(fgets($handle));	// C:\\danh_sach.txt\r\n	\r\n	$arr = unserialize(file_get_contents($ten_file));\r\n	xuat($arr);\r\n}\r\n\r\nfunction nam_hien_tai()\r\n{\r\n	return (int)date(\'Y\');\r\n}\r\n');
