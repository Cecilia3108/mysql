_DataGridView(object sender, EventArgs e)\r\n        {\r\n            this.selectRow(this.uiDataGridView.CurrentCell.RowIndex);\r\n        }\r\n\r\n        public void OnClick_Refresh(object sender, EventArgs e)\r\n        {\r\n            //this.FormGrid_Load(this, null);\r\n        }\r\n\r\n        private void OnClick_Add(object sender, EventArgs e)\r\n        {\r\n            var f = new FormAdd();\r\n            f.Owner = this;\r\n            f.Show();\r\n        }\r\n\r\n        private void OnClick_Edit(object sender, EventArgs e)\r\n        {\r\n            if (this.uiDataGridView.SelectedRows.Count <= 0)\r\n            {\r\n                MessageBox.Show(\"Bạn vui lòng chọn dòng bản ghi để sửa !\", \"Sửa Dữ Liệu\", MessageBoxButtons.OK, MessageBoxIcon.Warning);\r\n                return;\r\n            }\r\n\r\n            var f = new FormEdit();\r\n\r\n            // Gửi dữ liệu từ FormList sang FormEdit\r\n            f.Row = this.Row;\r\n            f.Owner = this;\r\n            f.Show();\r\n        }\r\n\r\n        private void OnClick_Delete(object sender, EventArgs e)\r\n        {\r\n            if (this.uiDataGridView.SelectedRows.Count <= 0)\r\n            {\r\n                MessageBox.Show(\"Bạn vui lòng chọn dòng bản ghi để xóa !\", \"Xóa Dữ Liệu\", MessageBoxButtons.OK, MessageBoxIcon.Warning);\r\n                return;\r\n            }\r\n\r\n            var yes_no = MessageBox.Show(\"Bạn có chắc muốn xóa ?\", \"Xác Nhận\", MessageBoxButtons.YesNo, MessageBoxIcon.Warning);\r\n\r\n\r\n            if (yes_no == DialogResult.Yes)\r\n            {\r\n                db.Entry(this.Row).State = System.Data.Entity.EntityState.Deleted;\r\n                db.SaveChanges();\r\n\r\n                this.FormGrid_Load(this, null);\r\n\r\n                MessageBox.Show(\"Đã Hoàn Tất Việc Xóa\", \"Thành Công\", MessageBoxButtons.OK, MessageBoxIcon.Information);\r\n            }\r\n        }\r\n\r\n        private void OnClick_Exit(object sender, EventArgs e)\r\n        {\r\n            Application.Exit();\r\n        }\r\n\r\n        private void OnMouseClick_DataGridView(object sender, MouseEventArgs e)\r\n        {\r\n            // Nếu nhấp chuột phải\r\n            if (e.Button == MouseButtons.Right)\r\n            {\r\n                // ...thì tạo menu ngữ cảnh \r\n                var cm = new ContextMenu();\r\n\r\n                // Lấy chỉ số của dòng để kiểm tra xem cú nhấp chuột phải có rơi trúng dòng nào không\r\n                int currentMouseOverRow = this.uiDataGridView.HitTest(e.X, e.Y).RowIndex;\r\n\r\n                // Nếu nhấp chuột phải vào đúng dòng\r\n                if (currentMouseOverRow >= 0)\r\n                {\r\n                    // thì bôi đen dòng đó\r\n                    this.selectRow(currentMouseOverRow);\r\n\r\n                    // Tạo các menu con(Sửa,Xóa) trên Menu ngữ cảnh\r\n                    // Có vẻ như không có thuộc tính Image để thêm biểu tượng cho MenuItem\r\n                    var menuItemEdit = new MenuItem(\"Sửa\");\r\n                    menuItemEdit.Click += OnClick_Edit;\r\n                    cm.MenuItems.Add(menuItemEdit);\r\n\r\n                    var menuItemDelete = new MenuItem(\"Xóa\");\r\n                    menuItemDelete.Click += OnClick_Delete;\r\n                    cm.MenuItems.Add(menuItemDelete);\r\n                }\r\n                else // ngược lại, nhấp chuột phải ra ngoài các dòng\r\n                {\r\n                    // thì tạo menu con Thêm Mới\r\n                    var menuItemAdd = new MenuItem(\"Thêm Mới\");\r\n                    menuItemAdd.Click += OnClick_Add;\r\n                    cm.MenuItems.Add(menuItemAdd);\r\n                }\r\n\r\n                // Hiển thị menu ngữ cảnh tại tọa độ nhấp chuột\r\n                cm.Show(this.uiDataGridView, new Point(e.X, e.Y));\r\n            }\r\n        }\r\n    }\r\n}\r\n'),
