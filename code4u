-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `code4mvc`
--

DROP TABLE IF EXISTS `code4mvc`;
CREATE TABLE IF NOT EXISTS `code4mvc` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `tag` varchar(255) NOT NULL,
  `content` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=69 DEFAULT CHARSET=utf8;

--
-- Đang đổ dữ liệu cho bảng `code4mvc`
--

INSERT INTO `code4mvc` (`id`, `name`, `tag`, `content`) VALUES
