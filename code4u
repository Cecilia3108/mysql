ách in %\r\nfunction row($obj, $i)\r\n{\r\n	printf(\"\\n| %-3s | %-8s | %3s | %-3.1f kg | %5s |\", \r\n		$i+1, $obj->sửa_Tên_Biến_1_đi, $obj->sửa_Tên_Biến_2_đi, $obj->sửa_Tên_Biến_3_đi, $obj->sửa_Tên_Biến_4_đi);\r\n	echo \"\\n+---------------------------------------+\";\r\n}\r\n\r\nfunction table()\r\n{\r\n	global $list;\r\n	\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(list_empty()) \r\n		return;\r\n\r\n	// Nếu đã nhập rồi thì tiến hành in ra màn hình\r\n	echo \"\\n List:\";\r\n\r\n	// Hiện các cột của bảng\r\n	col();\r\n\r\n	// sau đó hiện từng dòng bản ghi thông tin của mỗi đối tượng\r\n	for ($i=0; $i < count($list); $i++)\r\n	{\r\n		row($list[$i],$i);\r\n	}\r\n}\r\n\r\n\r\n\r\nfunction sort_list()\r\n{\r\n	global $list;\r\n	\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(list_empty()) \r\n		return;\r\n\r\n	// Nếu danh sách đã được nhập rồi thì tiến hành sắp xếp.\r\n	for ($i=0; $i<count($list); $i++)\r\n	{\r\n		// So sánh mỗi phần tử [i] với các phần tử\r\n		// còn lại từ [i+1] đến cuối\r\n		for ($j=$i+1; $j<count($list); $j++)\r\n		{\r\n			// Xếp theo tên tăng dần từ A-Z\r\n			$logic = strcmp($list[$i]->sửa_Cột_So_Sánh_đi, $list[$j]->sửa_Cột_So_Sánh_đi) < 0;\r\n			\r\n			// Xếp theo dữ liệu số giảm dần\r\n			//$logic = $list[$i]->sửa_Cột_So_Sánh_đi > $list[$j]->sửa_Cột_So_Sánh_đi);\r\n			\r\n			// Nếu quan hệ giữa [i] và [j] không thỏa mãn đề bài\r\n			if ($logic)\r\n			{\r\n				// thì tiến hành đổi chỗ:\r\n				$temp = $list[$i];		// gán [i] vào biến tạm\r\n				$list[$i] = $list[$j];	// gán [j] sang [i]\r\n				$list[$j] = $temp;		// gán biến tạm sang [j]\r\n			}\r\n		}\r\n	}\r\n\r\n	// và hiện danh sách (sau khi đã sắp xếp) ra màn hình\r\n	table();\r\n} // end sort()\r\n\r\nfunction analyze() {}\r\n\r\n// Tìm thú nuôi già nhất\r\nfunction find() {}\r\n\r\nfunction save()\r\n{\r\n	global $list;\r\n		\r\n	// Nếu kiểm tra danh sách thấy có vấn đề thì dừng hàm luôn\r\n	if(list_empty()) \r\n		return;\r\n	\r\n	echo \"\\n File name: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$file_name = rtrim(fgets($handle));	// C:\\list.txt\r\n\r\n	$fp=fopen($file_name,\"w\"); // mở file để viết (write) \r\n	\r\n	if($fp==NULL)\r\n	{\r\n		echo \"\\n Error-Cannot open file ! \\n\";\r\n	}\r\n	else\r\n	{\r\n		fwrite($fp, serialize($list));\r\n\r\n		echo \"\\n File saved successfully ! \\n\";\r\n	}\r\n\r\n	fclose($fp);\r\n}\r\n\r\nfunction open()\r\n{\r\n	global $list;\r\n		\r\n	echo \"\\n File name: \";\r\n	$handle = fopen (\"php://stdin\",\"r\");\r\n	$file_name = rtrim(fgets($handle));	// C:\\list.txt\r\n\r\n	$fp=fopen($file_name,\"r\"); // mở file để viết (write) \r\n	\r\n	if($fp==NULL)\r\n	{\r\n		echo \"\\n Error-Cannot open file ! \\n\";\r\n	}\r\n	else\r\n	{\r\n		$list = unserialize(file_get_contents($file_name));\r\n		table();\r\n	}\r\n	\r\n	fclose($fp);\r\n\r\n}\r\n\r\nfunction year()\r\n{\r\n	return (int)date(\'Y\');\r\n}\r\n\r\n'),
