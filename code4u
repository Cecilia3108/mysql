-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `code4js`
--

DROP TABLE IF EXISTS `code4js`;
CREATE TABLE IF NOT EXISTS `code4js` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `tag` text NOT NULL,
  `content` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=84 DEFAULT CHARSET=utf8;

--
-- Đang đổ dữ liệu cho bảng `code4js`
--

