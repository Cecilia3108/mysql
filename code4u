    mysql_output();\r\n\r\n    echo \"\\n Nhập khóa chính để sửa: \";\r\n    $fp=fopen(\"php://stdin\", \'r\');\r\n    $id = rtrim(fgets($fp));\r\n\r\n    echo \"\\n Nhập chuỗi: \";\r\n    $fp=fopen(\"php://stdin\", \'r\');\r\n    $biến_chuỗi = rtrim(fgets($fp));\r\n\r\n    echo \"\\n Nhập số nguyên: \";\r\n    $fp=fopen(\"php://stdin\", \'r\');\r\n    $biến_nguyên = (int)rtrim(fgets($fp));\r\n\r\n    echo \"\\n Nhập số thực: \";\r\n    $fp=fopen(\"php://stdin\", \'r\');\r\n    $biến_thực = (float)fgets($fp);\r\n\r\n    $sql = \"\r\n        UPDATE `_Tên_Bảng_` \r\n        SET `cột_1`=\'{$biến_1}\',\r\n            `cột_2`=\'{$biến_2}\',\r\n            `cột_3`=\'{$biến_3}\'\r\n        WHERE `_khóa_chính_`=\'{$id}\'\r\n    \";\r\n\r\n    db_q($sql);\r\n\r\n    echo \"\\n Đã sửa thành công !\";\r\n}\r\n\r\n// Xóa bản ghi\r\nfunction mysql_delete()\r\n{\r\n    mysql_output();\r\n\r\n    echo \"\\n Nhập khóa chính để xóa: \";\r\n    $fp=fopen(\"php://stdin\", \'r\');\r\n    $id = rtrim(fgets($fp));\r\n\r\n    $sql = \"DELETE FROM `_Tên_Bảng_` WHERE `_khóa_chính_`=\'{$id}\'\";\r\n\r\n    db_q($sql);\r\n\r\n    echo \"\\n Đã xóa thành công !\";\r\n}\r\n\r\nfunction mysql_check_list_empty($list)\r\n{\r\n	if(!is_array($list) || count($list) < 1)\r\n    {\r\n        echo \"\\n Lỗi-không có dữ liệu\";\r\n        return true;\r\n    }\r\n	\r\n	return false;\r\n}\r\n\r\nfunction mysql_sort()\r\n{\r\n    echo \" Sắp xếp...:\";\r\n    $list = db_q(\"SELECT * FROM `_Tên_Bảng_` ORDER BY `_Tên_Cột_Sắp_Xếp`\");\r\n	// sắp xếp giảm dần:\r\n	// $list = db_q(\"SELECT * FROM `_Tên_Bảng_` ORDER BY `_Tên_Cột_Sắp_Xếp` DESC\");\r\n	\r\n	if( mysql_check_list_empty($list) )\r\n        return;\r\n\r\n    mysql_columns();\r\n    foreach ($list as $obj)\r\n    {\r\n        mysql_row($obj);\r\n    }\r\n }\r\n\r\n\r\nfunction mysql_analyze()\r\n{\r\n    echo \"\\n Thống kê: \";\r\n\r\n    $sql = \"\r\n        SELECT `_Tên_Cột_Thống_Kê_`, \r\n               COUNT(`_khóa_chính_`) AS \'count\'\r\n        FROM `_Tên_Bảng_` \r\n        GROUP BY `_Tên_Cột_Thống_Kê_`\r\n    \";\r\n\r\n    $list = db_q($sql);\r\n\r\n	if( mysql_check_list_empty($list) )\r\n        return;\r\n\r\n    foreach($list as $obj)\r\n    {\r\n        echo \"\\n Có {$obj[\'count\']} bạn sinh năm {$obj[\'_Tên_Cột_Thống_Kê_\']}\";\r\n    }\r\n}\r\n\r\nfunction mysql_search()\r\n{\r\n    echo \"\\n Nhập từ khóa để tìm: \";   \r\n    $fp=fopen(\"php://stdin\", \'r\');\r\n    $keyword = rtrim(fgets($fp));\r\n    \r\n    $sql = \"SELECT * FROM `_Tên_Bảng_` WHERE `_Tên_Cột_Tìm_Kiếm_` LIKE \'%{$keyword}%\'\";\r\n\r\n    $list = db_q($sql);\r\n	\r\n	if( mysql_check_list_empty($list) )\r\n        return;\r\n	\r\n    printf(\"\\n Tìm thấy %s đối tượng khớp với \'%s\'\", count($list), $keyword);\r\n    \r\n    mysql_columns();\r\n    foreach ($list as $obj)\r\n    {\r\n        mysql_row($obj);\r\n    }\r\n}'),
