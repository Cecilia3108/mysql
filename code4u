ize();\r\n        self.uiGrid.SetSelectionMode( wx.grid.Grid.wxGridSelectRows );\r\n    #} end load\r\n    \r\n    def OnMenuSelection_Exit( self, event ):\r\n    #{\r\n        self.Destroy();\r\n    #}\r\n    \r\n    def OnSelect_MenuAbout( self, event ):\r\n    #{\r\n        wx.MessageBox(\"Cảm ơn bạn đã dùng phần mềm ! \\n (C)2020 Lê Minh Họa\",\"About Me\");\r\n    #}\r\n    \r\n    def OnButtonClick_Add( self, event ):\r\n    #{\r\n        ui_add = UiAddCode();\r\n        ui_add.Show();\r\n    #}\r\n    \r\n    # @todo thay message box bằng cái gì đó nghiêm trọng hơn\r\n    def OnButtonClick_Edit( self, event ):\r\n    #{\r\n        # Nếu Grid không có bản ghi nào\r\n        if (self.uiGrid.GetNumberRows() < 1):\r\n            wx.MessageBox(\"Không có bản ghi nào\", \"Lỗi\");\r\n            \r\n        # Nếu không có dòng nào được chọn (bôi đen) thì báo lỗi\r\n        if (self.row_index_selected < 0):\r\n            wx.MessageBox(\"Bạn vui lòng chọn bản ghi để sửa\", \"Lỗi\");\r\n            \r\n        pk = self.uiGrid.GetCellValue(self.row_index_selected,0);\r\n        \r\n        ui_edit = UiEditCode(pk);\r\n        ui_edit.Show();\r\n    #}\r\n    \r\n    def OnButtonClick_Delete( self, event ):\r\n    #{\r\n        # Nếu Grid không có bản ghi nào\r\n        if (self.uiGrid.GetNumberRows() < 1):\r\n            wx.MessageBox(\"Không có bản ghi nào\", \"Lỗi\");\r\n            \r\n        # Nếu không có dòng nào được chọn (bôi đen) thì báo lỗi\r\n        if (self.row_index_selected < 0):\r\n            wx.MessageBox(\"Bạn vui lòng chọn bản ghi để sửa\", \"Lỗi\");\r\n            \r\n        pk = self.uiGrid.GetCellValue(self.row_index_selected,0);\r\n        \r\n        answer = wx.MessageBox(\"Bạn có chắc muốn xóa ?\", \"Xác nhận\", wx.YES_NO, self);\r\n        \r\n        if (answer == wx.YES):\r\n        #{\r\n            sql = \"DELETE FROM `sửa_Tên_Bảng` WHERE `sửa_Khóa_Chính`=\'%s\'\" % pk;\r\n            db_q(sql);\r\n            wx.MessageBox(\"Đã hoàn 