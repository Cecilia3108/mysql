(27, 'FormMain.cs (mã điều khiển)', 'FormMain.cs (code behind)', 'using System;\r\nusing System.Collections.Generic;\r\nusing System.ComponentModel;\r\nusing System.Data;\r\nusing System.Drawing;\r\nusing System.Linq;\r\nusing System.Text;\r\nusing System.Threading.Tasks;\r\nusing System.Windows.Forms;\r\n\r\nnamespace sửa_Tên_Dự_Án_đi\r\n{\r\n    public partial class FormMain : Form\r\n    {\r\n        public FormMain()\r\n        {\r\n            InitializeComponent();\r\n        }\r\n\r\n        string Action { get; set; }\r\n        DBEntities db = new DBEntities();\r\n        TÊN_BẢNG_trên_GridView Row { get; set; }\r\n\r\n        protected override void OnShown(EventArgs e)\r\n        {\r\n            base.OnShown(e);\r\n\r\n            this.uiDataGridView.Click += OnClick_DataGridView;\r\n            this.uiButton_Query.Click += OnClick_Query;\r\n            this.uiButton_ExportXML.Click += OnClick_ExportXML;\r\n            this.uiButton_Save.Click += OnClick_Save;\r\n            this.uiButton_Insert.Click += OnClick_Insert;\r\n            this.uiButton_Update.Click += OnClick_Update;\r\n            this.uiButton_Delete.Click += OnClick_Delete;\r\n\r\n            this.uiDataGridView.SelectionMode = DataGridViewSelectionMode.FullRowSelect;\r\n            this.uiDataGridView.ReadOnly = true;\r\n            this.uiDataGridView.AllowUserToAddRows = false;\r\n\r\n            this.selectRow(0);\r\n        }\r\n\r\n        public void selectRow(int index)\r\n        {\r\n            this.uiDataGridView.ClearSelection();\r\n            this.uiDataGridView.Rows[index].Selected = true;\r\n            this.uiDataGridView.CurrentCell = this.uiDataGridView.Rows[index].Cells[0];\r\n\r\n            this.uiButton_Update.Enabled = true;\r\n            this.uiButton_Delete.Enabled = true;\r\n\r\n            this.Row = this.db.TÊN_BẢNG_SỐ_NHIỀU_trên_GridView.ToList().ElementAt(index);\r\n\r\n            this.uiComboBox_.SelectedValue = this.Row.KHÓA_CHÍNH_BẢNG_trên_ComboBox;\r\n            this.uiTextBox_1.Text = this.Row.CỘT_1;\r\n            this.uiTextBox_2.Text = this.Row.CỘT_2;\r\n            this.uiTextBox_3.Text = this.Row.CỘT_3;\r\n\r\n            this.enableFormRow(false);\r\n        }\r\n\r\n        void enableFormRow(bool on = true)\r\n        {\r\n            this.uiButton_Save.Enabled = on;\r\n            this.uiButton_Insert.Enabled = !on;\r\n            this.uiButton_Update.Enabled = !on;\r\n            this.uiButton_Delete.Enabled = !on;\r\n\r\n            this.uiComboBox_.Enabled = on;\r\n            this.uiTextBox_1.Enabled = on;\r\n            this.uiTextBox_2.Enabled = on;\r\n            this.uiTextBox_3.Enabled = on;\r\n\r\n        }\r\n\r\n        private void OnClick_DataGridView(object sender, EventArgs e)\r\n        {\r\n            this.selectRow(this.uiDataGridView.CurrentCell.RowIndex);\r\n        }\r\n\r\n        private void OnClick_Query(object sender, EventArgs e)\r\n        {\r\n            this.FormMain_Load(this, null);\r\n        }\r\n\r\n        private void OnClick_Insert(object sender, EventArgs e)\r\n        {\r\n            this.uiTextBox_1.Clear();\r\n            this.uiTextBox_2.Clear();\r\n            this.uiTextBox_3.Clear();\r\n\r\n            this.Action = \"Add\";\r\n            this.enableFormRow();\r\n        }\r\n\r\n        private void OnClick_Update(object sender, EventArgs e)\r\n        {\r\n            if (this.uiDataGridView.SelectedRows.Count <= 0)\r\n            {\r\n                MessageBox.Show(\"Bạn vui lòng chọn dòng bản ghi để sửa !\", \"Sửa Dữ Liệu\", MessageBoxButtons.OK, MessageBoxIcon.Warning);\r\n                return;\r\n            }\r\n\r\n            this.selectRow(this.uiDataGridView.CurrentCell.RowIndex);\r\n            this.Action = \"Edit\";\r\n            this.enableFormRow();// bật form lên để sửa\r\n        }\r\n\r\n        private void OnClick_Delete(object sender, EventArgs e)\r\n        {\r\n            if (this.uiDataGridView.SelectedRows.Count <= 0)\r\n            {\r\n                MessageBox.Show(\"Bạn vui lòng chọn dòng bản ghi để xóa !\", \"Xóa Dữ Liệu\", MessageBoxButtons.OK, MessageBoxIcon.Warning);\r\n                return;\r\n            }\r\n\r\n            var yes_no = MessageBox.Show(\"Bạn có chắc muốn xóa ?\", \"Xác Nhận\", MessageBoxButtons.YesNo, MessageBoxIcon.Warning);\r\n\r\n\r\n            if (yes_no == DialogResult.Yes)\r\n            {\r\n                db.Entry(this.Row).State = System.Data.Entity.EntityState.Deleted;\r\n                db.SaveChanges();\r\n\r\n                this.FormMain_Load(this, null);\r\n\r\n                MessageBox.Show(\"Đã Hoàn Tất Việc Xóa\", \"Thành Công\", MessageBoxButtons.OK, MessageBoxIcon.Information);\r\n            }\r\n        }\r\n\r\n        private void OnClick_Save(object sender, EventArgs e)\r\n        {\r\n            if (this.Action == \"Add\")\r\n            {\r\n                db.TÊN_BẢNG_SỐ_NHIỀU_trên_GridView.Add(new TÊN_BẢNG_trên_GridView()\r\n                {\r\n                    KHÓA_CHÍNH_BẢNG_trên_ComboBox = Convert.ToInt32(this.uiComboBox_.SelectedValue),\r\n                    CỘT_1 = this.uiTextBox_1.Text,\r\n                    CỘT_2 = this.uiTextBox_2.Text,\r\n                    CỘT_3 = this.uiTextBox_3.Text,\r\n                    GioiTinh = true,\r\n                    NSinh = new DateTime(1990, 08, 28),\r\n                    Password = \"123456\"\r\n\r\n                });\r\n                db.SaveChanges();\r\n\r\n                this.FormMain_Load(this, null);\r\n                this.selectRow(this.uiDataGridView.RowCount - 1);\r\n\r\n                MessageBox.Show(\"Đã hoàn tất việc thêm mới !\", \"Thành Công\", MessageBoxButtons.OK, MessageBoxIcon.Information);\r\n            }\r\n            else     // this.Action == \"Edit\"\r\n            {\r\n                this.Row.KHÓA_CHÍNH_BẢNG_trên_ComboBox = Convert.ToInt32(this.uiComboBox_.SelectedValue);\r\n                this.Row.CỘT_1 = this.uiTextBox_1.Text;\r\n                this.Row.CỘT_2 = this.uiTextBox_2.Text;\r\n                this.Row.CỘT_3 = this.uiTextBox_3.Text;\r\n\r\n                db.Entry(this.Row).State = System.Data.Entity.EntityState.Modified;\r\n                db.SaveChanges();\r\n\r\n                this.FormMain_Load(this, null);\r\n                this.selectRow(this.uiDataGridView.CurrentCell.RowIndex);\r\n\r\n                MessageBox.Show(\"Đã hoàn tất việc sửa !\", \"Thành Công\", MessageBoxButtons.OK, MessageBoxIcon.Information);\r\n\r\n            }\r\n\r\n            this.enableFormRow(false);\r\n        }\r\n\r\n        private void OnClick_ExportXML(object sender, EventArgs e)\r\n        {\r\n            var sfd = new SaveFileDialog();\r\n            sfd.Filter = \"XML Files|*.xml\";\r\n\r\n            if (sfd.ShowDialog() == DialogResult.OK)\r\n            {\r\n                var serialiser = new System.Xml.Serialization.XmlSerializer(typeof(List<TÊN_BẢNG_trên_GridView>));\r\n\r\n                var filestream = new System.IO.StreamWriter(sfd.FileName);\r\n\r\n                serialiser.Serialize(filestream, this.db.TÊN_BẢNG_SỐ_NHIỀU_trên_GridView.ToList());\r\n\r\n                filestream.Close();\r\n            }\r\n\r\n            MessageBox.Show(\"Đã hoàn tất việc xuất file XML.\", \"Thành Công\", MessageBoxButtons.OK, MessageBoxIcon.Information);\r\n        }\r\n\r\n\r\n\r\n    }\r\n}\r\n'),
